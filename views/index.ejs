<html>
  <head>
    <% include partials/head %>
  </head>
  <body>
    <header>
      <% include partials/header %>
    </header>
<% tracks.forEach(function (track) { %>
    <section>
      <a href="<%= artist.name %>"><img src="<%= track.image[2]['#text'] %>" alt="" width='174' height='174'></a>
      <div class="">
        <h2><%= track.name %></h2>
        <p><%= track.artist.name %></p>
        <p><%= track.playcount %></p>

      </div>
    </section>
<% }) %>
    <script type="module" src="js/main.js"></script>

    <footer>
      <% include partials/footer%>
    </footer>
  </body>

</html>
